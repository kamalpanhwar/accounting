extends layout

block content
  h1= title

  ul
    - user_list.sort(function(a,b) { let textA = a.title.toUpperCase(); let textB = b.title.toUpperCase(); return(textA < textB) ? -1 : (textA > textB) ? 1 : 0; } );
    each user in user_list
      li
        a(href=user.url) #{user.name}
 
    else
      li There are no users.
